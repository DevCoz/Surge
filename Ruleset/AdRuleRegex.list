# 网易云音乐
URL-REGEX,"^http:\/\/[\s\S]*\/eapi\/ad\/"
URL-REGEX,"^http:\/\/p\d\.music\.126\.net\/\w+==\/\d+\.jpg$"
URL-REGEX,"^http:\/\/iad.*mat\.[a-z]*\.12[67]\.net/\w+\.(jpg|mp4)$"
AND,((OR,((USER-AGENT,NeteaseMusic*),(USER-AGENT,%E7%BD%91%E6%98%93%E4%BA%91%E9%9F%B3%E4%B9%90*))), (URL-REGEX,^https?:\/\/.*?\/e?api\/ad))

# 喜马拉雅
URL-REGEX,"^http:\/\/[\s\S]*\/ting\/[a-z]*\/ts-\d+"
URL-REGEX,"^http:\/\/.+\/(soundPatch|ting).*\/ts"
AND,((USER-AGENT,ting*), (URL-REGEX,(^http:\/\/.+\/soundPatch|ad.*\.ximalaya\.com)))

# 字节跳动
