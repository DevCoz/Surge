# URL-REGEX

#Privacy
//URL-REGEX,^https?:\/\/(\w+\.)?(adclick|ads([0-9]+)?|adserver|adformat|analysis|banners?|click|counter|log|log-?\w+?|pagead|stat|stats|statis|trace|track|tracking|uniad)\.\w+\.(com|cn|org|info|io|net)

# > 腾讯视频
USER-AGENT,*VBQADCommonKit*
AND,((DOMAIN-KEYWORD,splash), (DOMAIN-KEYWORD,gtimg))

# 高德地图
DOMAIN,optimus-ads.amap.com
URL-REGEX,"^https?:\/\/m5\.amap\.com\/ws\/valueadded\/alimama\/splash"

# 网易云音乐
URL-REGEX,"^http:\/\/[\s\S]*\/eapi\/ad\/"
URL-REGEX,"^http:\/\/p\d\.music\.126\.net\/\w+==\/\d+\.jpg$"
URL-REGEX,"^http:\/\/iad.*mat\.[a-z]*\.12[67]\.net/\w+\.(jpg|mp4)$"
AND,((OR,((USER-AGENT,NeteaseMusic*),(USER-AGENT,%E7%BD%91%E6%98%93%E4%BA%91%E9%9F%B3%E4%B9%90*))), (URL-REGEX,^https?:\/\/.*?\/e?api\/ad))

# 喜马拉雅
URL-REGEX,"^http:\/\/[\s\S]*\/ting\/[a-z]*\/ts-\d+"
URL-REGEX,"^http:\/\/.+\/(soundPatch|ting).*\/ts"
AND,((USER-AGENT,ting*), (URL-REGEX,(^http:\/\/.+\/soundPatch|ad.*\.ximalaya\.com)))

# 字节跳动
# *.snssdk.com, *.amemv.com, *.musical.ly, *.pstatp.com
URL-REGEX,"^https?:\/\/[\s\S]*\.(snssdk|amemv)\.com\/api\/ad\/"
URL-REGEX,"^https?:\/\/sf.*-ttcdn-tos\.pstatp\.com\/obj\/ad"

# pixiv
AND,((USER-AGENT,pixiv*), (NOT,((DOMAIN-KEYWORD,pixiv))))
